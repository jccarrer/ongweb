      <!-- Content Header (Page header) -->
      <section class="content-header">
        <h1>
          Mis Cursos
          <small>Activos</small>
        </h1>
        <ol class="breadcrumb">
          <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
          <!--li><a href="#">Layout</a></li>
          <li class="active">Top Navigation</li-->
        </ol>
      </section>




      <!-- Main content -->
      <section class="content">




 <div class="row">
				 

				  <div class="col-lg-12 col-xs-12">		
					 Proyecto: {{ proyecto.nombre }} 
				  </div>				  
				  <div class="col-lg-12 col-xs-12">
					<p>Mi equipo es:</p>
				  </div>
				  {% for cargos_proyectos in cargos_proyectos %}
					{% if cargos_proyectos.proyecto ==   proyecto.nombre   %}
						
						<div class="col-lg-12 col-xs-12">
							<p>- {{ cargos_proyectos.cargo }}: {{ cargos_proyectos.nombre }} </p>
						</div>
					{% endif %}
                  {% endfor %}				



					







               
                  
</div>



					{% for resultado in resultados %}
						{% if resultado.proyecto ==   proyecto.nombre   %}

								<div >
									<p>{{resultado.nombre}}:  {{resultado.descripcion}} - presupesto de ${{resultado.presupuesto}}</p> 
										<ul>
										{% for indicadores in indicadores %}
											{% if indicadores.resultados ==  resultado.nombre   %}
												<li> Indicador: {{indicadores.detalle}} </li>
													<ul>
													{% for actividades in actividades %}
														{% if actividades.indicadores ==  indicadores.detalle   %}
															<li> Actividad: {{actividades.nombre}} - Docente: {{actividades.nombresApellidosDocente}} - <a href="#">Beneficiarios</a></li>
														{% endif %}
													{% endfor %}
													</ul>
											{% endif %}
										{% endfor %}
										</ul>
								</div>
						{% endif %}
					{% endfor %}





      <!-- /.row (main row) -->
				  


		<!-- Main content -->
		</section>


    </div>
    <!-- /.container -->
  </div>
