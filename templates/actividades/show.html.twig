<!DOCTYPE html>

<title>Actividades</title>

{% block body %}
    <h1>Actividades</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ actividade.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ actividade.nombre }}</td>
            </tr>
            <tr>
                <th>Fecha_inicio</th>
                <td>{{ actividade.fechaInicio }}</td>
            </tr>
            <tr>
                <th>Fecha_final</th>
                <td>{{ actividade.fechaFinal }}</td>
            </tr>
            <tr>
                <th>Identificacion_docente</th>
                <td>{{ actividade.identificacionDocente }}</td>
            </tr>
            <tr>
                <th>Nombres_apellidos_docente</th>
                <td>{{ actividade.nombresApellidosDocente }}</td>
            </tr>
            <tr>
                <th>Presupuesto_proyectado</th>
                <td>{{ actividade.presupuestoProyectado }}</td>
            </tr>
            <tr>
                <th>Presupuesto_ejecutado</th>
                <td>{{ actividade.presupuestoEjecutado }}</td>
            </tr>
            <tr>
                <th>Presupuesto_avance</th>
                <td>{{ actividade.presupuestoAvance }}</td>
            </tr>
            <tr>
                <th>Meta</th>
                <td>{{ actividade.meta }}</td>
            </tr>
            <tr>
                <th>Avances</th>
                <td>{{ actividade.avances }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ actividade.estado }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('actividades_index') }}">back to list</a>

    <a href="{{ path('actividades_edit', {'id': actividade.id}) }}">edit</a>

    {{ include('actividades/_delete_form.html.twig') }}
{% endblock %}
